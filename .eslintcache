tificador]);\n\n  //Consulta el usuario activo actualmente\n\n  const Usuario = async () => {\n    try {\n      const headers = { \"x-auth-token\": token };\n      const { data } = await axios.get(\"auth\", {\n        headers,\n      });\n      setUsuario(data.usuario);\n      setProveedor(data.usuario.usuario);\n    } catch (error) {\n      console.log(error);\n    }\n  };\n\n  //Consulta de Publicaiones\n\n  const Publicacion = async () => {\n    try {\n      const { data } = await axios.get(\"home\");\n      setPublicaciones(data);\n    } catch (error) {\n      console.log(error);\n    }\n  };\n  //Funcion de Eliminar Publicacion\n  const Delete = async () => {\n    try {\n      const headers = { \"x-auth-token\": token };\n      await axios.delete(`home/${identificador}`, { headers });\n      window.location.href = \"/\";\n    } catch (error) {\n      console.log(error);\n    }\n  };\n\n  //Aqui se hacemos el map de todos las publicaciones\n  const MapDataBase =\n    (publicaciones.length === 0 && (\n      <div className=\"d-flex justify-content-center align-items-center\">\n        <Spinner animation=\"border\" variant=\"primary\" />\n      </div>\n    )) ||\n    publicaciones.map((date, i) => (\n      <div className=\"CardDiv\" key={i}>\n        <Card className=\"CardPublica\">\n          <div className=\"d-flex justify-content-between\">\n            <Card.Header>\n              {date.proveedor}\n            </Card.Header>\n            {date.creador === usuario._id && (\n              <div>\n                <div>\n                  <NavDropdown title=\"Opciones\" id=\"basic-nav-dropdown\">\n                    <NavDropdown.Item>Editar</NavDropdown.Item>\n                    <NavDropdown.Item\n                      onClick={() => setIdentificador(date._id)}\n                    >\n                      Eliminar\n                    </NavDropdown.Item>\n                  </NavDropdown>\n                </div>\n              </div>\n            )}\n          </div>\n          <div>\n            <div className=\"m-2 descripcionPublicacion\">{date.titulo}</div>\n            <Card.Body>\n              <div>\n                <i>{date.contenido}</i>\n              </div>\n            </Card.Body>\n          </div>\n        </Card>\n      </div>\n    ));\n\n  //Aqui hacemos el map y compara con el id del producto y el del usuario\n\n  const MapComparatePublic =\n    (publicaciones.length === 0 && (\n      <div className=\"d-flex justify-content-center align-items-center\">\n        <Spinner animation=\"border\" variant=\"primary\" />\n      </div>\n    )) ||\n    publicaciones.map((date, i) => (\n      <div>\n        {date.creador === usuario._id && (\n          <div className=\"CardDiv\" key={i}>\n            <Card className=\"CardPublica\">\n              <div className=\"d-flex justify-content-between\">\n                <div>\n                  <Card.Header>\n                    {date.proveedor}\n                  </Card.Header>\n                </div>\n                <div>\n                  <NavDropdown title=\"Opciones\" id=\"basic-nav-dropdown\">\n                    <NavDropdown.Item>Editar</NavDropdown.Item>\n                    <NavDropdown.Item\n                      onClick={() => setIdentificador(date._id)}\n                    >\n                      Eliminar\n                    </NavDropdown.Item>\n                  </NavDropdown>\n                </div>\n              </div>\n              <div>\n                <div className=\"m-2 descripcionPublicacion\">{date.titulo}</div>\n              </div>\n              <Card.Body>\n                <Card.Text>{date.contenido}</Card.Text>\n              </Card.Body>\n            </Card>\n          </div>\n        )}\n      </div>\n    ));\n  return {\n    MapComparatePublic,\n    publicaciones,\n    MapDataBase,\n    proveedor,\n    usuario,\n  };\n};\nexport default UseHome;\n","/home/conteo/Documentos/Full Stack/Vlog-Conteo/vlog-frontend/src/components/Publicacion/